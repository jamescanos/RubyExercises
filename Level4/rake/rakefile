require 'similarity'
content = File.read("compare.txt") # Reads File
lines = content.split("\n") # divide el contenido en líneas

# recorre las líneas y las imprime
lines.each do |line|
  #puts line
  element = line.split(",")
  print Similarity.similar("#{element[0]}", "#{element[1]}")
  print "\n"
end